{
  "name": "Easicoin",
  "displayName": "Easicoin 交易所",
  "description": "Easicoin 交易所晒单收益图模板",
  "version": "1.0.0",
  "priceSource": "binance",
  "supportedPairs": [
    "ETHUSDT",
    "BTCUSDT"
  ],
  "defaultPair": "ETHUSDT",
  "template": {
    "width": 908,
    "height": 1280,
    "backgroundPattern": "{pair}-background.jpg"
  },
  "styling": {
    "profitColor": "#21C07C",
    "lossColor": "#F6465D",
    "textShadow": "0 0 1px rgba(0,0,0,.8)"
  },
  "displayTexts": {
    "direction": {
      "long": "做多",
      "short": "做空",
      "open_long": "开多",
      "open_short": "开空",
      "close_long": "平多",
      "close_short": "平空"
    },
    "leverage": "{lev}x",
    "yield": "{value}%"
  },
  "dynamicColors": {
    "direction": {
      "open_long": "#21C07C",
      "open_short": "#F6465D",
      "close_long": "#F6465D",
      "close_short": "#21C07C",
      "long": "#21C07C",
      "short": "#F6465D"
    }
  },
  "qrcode": {
    "baseUrl": "https://www.easicoinx.com/account/register/?inviteCode=",
    "defaultRefCode": "HAJIMI"
  }
}
